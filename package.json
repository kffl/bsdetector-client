{
	"name": "bsdetector-frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"core-js": "^2.6.5",
		"lodash": "^4.17.15",
		"vue": "^2.6.10",
		"vue-markdown": "^2.2.4",
		"vue-router": "^3.1.6",
		"vuetify": "^2.2.11"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.12.0",
		"@vue/cli-plugin-eslint": "^4.3.1",
		"@vue/cli-service": "^3.12.0",
		"@vue/eslint-config-standard": "^5.1.2",
		"babel-eslint": "^10.1.0",
		"cypress": "^4.3.0",
		"eslint": "^6.8.0",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.0",
		"eslint-plugin-vue": "^6.2.2",
		"netlify-plugin-cypress": "^1.3.3",
		"sass": "^1.26.3",
		"sass-loader": "^8.0.2",
		"vue-cli-plugin-pug": "~1.0.7",
		"vue-cli-plugin-vuetify": "^2.0.5",
		"vue-template-compiler": "^2.6.10",
		"vuetify-loader": "^1.3.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"plugins": [
			"cypress"
		],
		"extends": [
			"plugin:vue/essential",
			"@vue/standard",
			"plugin:cypress/recommended"
		],
		"rules": {
			"indent": [
				"error",
				"tab"
			],
			"no-tabs": "off",
			"semi": [
				"error",
				"always"
			],
			"comma-dangle": [
				"error",
				"always-multiline"
			]
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"lintOnSave": "error"
}